{
  "name": "flowbitz",
  "version": "1.0.0",
  "description": "Interactive components library for Webflow - React Showcase",
  "type": "module",
  "main": "dist/flowbitz.umd.js",
  "module": "dist/flowbitz.es.js",
  "exports": {
    ".": {
      "import": "./dist/flowbitz.es.js",
      "require": "./dist/flowbitz.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --config vite.config.library.js",
    "build:app": "vite build",
    "build:library": "vite build --config vite.config.library.js",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "webflow",
    "gsap",
    "animation",
    "components",
    "react",
    "showcase",
    "interactive"
  ],
  "author": "Slabpixel Studio",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "googleapis": "^159.0.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.542.0",
    "ogl": "^1.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.8.0",
    "react-syntax-highlighter": "^15.6.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.24.0",
    "vercel": "^48.0.0",
    "vite": "^7.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Slabpixel/FlowBitz"
  }
}
