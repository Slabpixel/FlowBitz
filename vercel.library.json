{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/v([0-9]+)\\.([0-9]+)\\.([0-9]+)/(.*)",
      "dest": "/$4",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "X-Version": "$1.$2.$3",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/latest/(.*)",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600",
        "X-Version": "latest",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/beta/(.*)",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=300",
        "X-Version": "beta",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/flowbitz.umd.js",
      "dest": "/latest/flowbitz.umd.js",
      "status": 301,
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/flowbitz.es.js",
      "dest": "/latest/flowbitz.es.js",
      "status": 301,
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/404.html",
      "status": 404
    }
  ]
}
